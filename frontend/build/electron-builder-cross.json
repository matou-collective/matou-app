{
  "appId": "org.matou.app",
  "productName": "Matou",
  "artifactName": "matou-${version}.${ext}",
  "directories": {
    "app": "dist/electron/UnPackaged",
    "output": "dist/electron/Packaged"
  },
  "extraResources": [
    { "from": "../backend/bin/", "to": "backend/" },
    { "from": "src-electron/icons/", "to": "icons/" }
  ],
  "mac": {
    "target": "zip",
    "identity": null,
    "icon": "src-electron/icons/icon.png"
  },
  "win": {
    "target": "nsis",
    "icon": "src-electron/icons/icon.png"
  }
}
