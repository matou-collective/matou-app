# Any-sync client configuration for PRODUCTION network
# Copy to client-production.yml and update with your production network values
#
# Used when running: MATOU_ENV=production go run ./cmd/server
#
# Get these values from your any-sync infrastructure after running:
#   make up (on production server)
#   Copy from: etc/client.yml
#
# The id, networkId, and peerIds are generated by the anyconf tool
# during network initialization and must match your production nodes.

id: ""           # Network ID (from coordinator config)
networkId: ""    # Network ID string (from coordinator)
nodes:
  - peerId: ""   # Get from production coordinator
    addresses:
      - any-sync.example.com:1001
    types:
      - tree
  - peerId: ""
    addresses:
      - any-sync.example.com:1002
    types:
      - tree
  - peerId: ""
    addresses:
      - any-sync.example.com:1003
    types:
      - tree
  - peerId: ""
    addresses:
      - any-sync.example.com:1004
    types:
      - coordinator
  - peerId: ""
    addresses:
      - any-sync.example.com:1005
    types:
      - file
  - peerId: ""
    addresses:
      - any-sync.example.com:1006
    types:
      - consensus
